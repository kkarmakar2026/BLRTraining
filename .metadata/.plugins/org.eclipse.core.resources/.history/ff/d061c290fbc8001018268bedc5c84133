  package com.mphasis;

import java.util.function.Function;

public class DemoOnStaticMethodReferences {
	
	// classname::methodname
	// objname::method
	
	static class NumberUtil{
		public static int squareNum(int num) {
			return num*num;
		}
	}
	
	public static void main(String[] args) {
		
		Function<Integer,Integer> squareFunction=(num)->NumberUtil.squareNum(5);
		
	}

}
