		public class Demo {
		    public static void main(String[] args) {
		        Integer nums[] = {4, 5, 6, 7, 8, 9, 0};   // use Integer[], not int[]
		        String strs[] = {"str", "str2", "str3", "str4"};

		        ArrayPrinter<Integer> ap1 = new ArrayPrinter<>(nums);
		        ap1.printArray();   // Output: 4 5 6 7 8 9 0

		        ArrayPrinter<String> ap2 = new ArrayPrinter<>(strs);
		        ap2.printArray();   // Output: str str2 str3 str4
		    }
		}

	        
	} 
	
