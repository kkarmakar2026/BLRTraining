package com.mphasis;
import java.util.Scanner;

public class First5 {

	
	public static void main (String a[]) {
		System.err.println("Enter a number");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int reversenum = 0;
		while (n>=0) {
			int temp = n%10;
			n = n/10;
			
			 reversenum = temp*10+reversenum;
		}
		
		System.out.println(reversenum);
	}
}
