package com.mphasis;

public class First {
    public static void main(String[] args) {
        String phno = "7903780988";
        if(phno.length()==10 && phno.charAt(0)==6 ||phno.charAt(0)==7||phno.charAt(0)==8||phno.charAt(0)==9) {
        	System.out.println("Valid Number");
        }
        System.out.println("Invalid Number");
        
        
        
        
    }
}
