package com.mphasis;



public class QueueUsingArray {
	
	 static int arr [] = {10,20,30,40};

	public static int front  = -1;
	public static int rear = -1;
	
	public static boolean isEmpty() {
		if(rear == -1 && front ==-1)
			return true;
		else 
			return false;
	}
	 public static  boolean isFull() {
		 if (rear == arr.length)
			 return true;
		 return false;
	 }
	
}
