package com.mphasis;

public class DemoException {

    public static void main(String[] args) {
        int a = 5, b = 0;
        try {
        	System.out.println(a / b);
		} catch (Exception e) {
			// TODO: handle exception
			e.printStackTrace();
		}
        finally {
			System.out.println("Finally bloked called");
		}
        System.out.println("Application closed successfully");
        
    }
}
