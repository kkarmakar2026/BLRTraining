package com.mphasis;

public class BankingApp {

    public static void main(String[] args) {
        
        // Using default constructor + setters
        Account acc1 = new Account();
        acc1.setAccountNumber("1234567890");
        acc1.setIfscCode("IFSC00123");
        acc1.setLocation("Chennai");
        acc1.setBalance(50000.75);
        acc1.setPin(1234);
        acc1.setAccHolderName("Balai");

        System.out.println("Account 1 Details:");
        System.out.println(acc1);

        // Using parameterized constructor
        Account acc2 = new Account("9876543210", "IFSC00987", "Bangalore", 75000.50, 5678, "KJjkkjj");

        System.out.println("Account 2 Details:");
        System.out.println(acc2);
    }
}
