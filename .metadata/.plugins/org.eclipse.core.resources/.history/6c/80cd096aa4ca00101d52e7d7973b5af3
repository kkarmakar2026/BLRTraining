package com.mphasis;

import java.util.Arrays;
import java.util.Iterator;
import java.util.LinkedList;

public class DemoOnLinkedList {

    public static void main(String[] args) {
        
        // Define an array to convert into LinkedList
        Integer[] irr = {10, 20, 30, 40, 50};

        // Create LinkedList from array
        LinkedList<Integer> ll = new LinkedList<>(Arrays.asList(irr));

        // Add extra elements if needed
        ll.add(56);
        ll.add(32);
        ll.add(26);
        ll.add(86);

        // Use descendingIterator to traverse backwards
        Iterator<Integer> itr = ll.descendingIterator();
        
        while (itr.hasNext()) {
            System.out.println(itr.next());
        }
    }
}
