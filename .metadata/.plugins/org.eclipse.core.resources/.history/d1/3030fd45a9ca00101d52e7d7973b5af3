package com.mphasis;

import java.util.Vector;

public class DemoOnVector {

	public static void main(String[] args) {
		
		Vector<Integer> vector = new Vector<Integer>();
		System.out.println(vector.capacity());
		for (int i=1; i<=10;i++)
			vector.add(i);
		System.out.println(vector.capacity());
		vector.add(11);
		System.out.println(vector.capacity());
	}
}
