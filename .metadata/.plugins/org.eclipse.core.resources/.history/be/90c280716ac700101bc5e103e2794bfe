package com.mphasis;

public class DemoException {

    public static void main(String[] args) {
        int a = 5, b[] = {4, 2, 6, 3, 0};
        String str ="";

        try {
            for (int i = 0; i <= b.length; i++) {
                System.out.println("Result of " + a + "/" + b[i] + " = " + (a / b[i]));
            }
        }
        catch (Exception e) {
			// TODO: handle exception
        	str.equals("Hello");
        }
    }
}
    
        
