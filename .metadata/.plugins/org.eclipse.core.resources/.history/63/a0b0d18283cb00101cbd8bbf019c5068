package com.mphasis;

public class SelectionSort {

	public static void main(String[] args) {
		int arr[] = {23, 10, 15, 45, 62, 27, 9, 89, 13};
		int n = arr.length;
		for(int i =0;i<n;i++) {
			int mi = i;
			for(int j = i+1; j<n;j++) {
				mi = j;
			}
			int temp = arr[i];
			arr[i] = arr[mi];
			arr[mi] = temp;
		}
			for(int i = 0;i<arr.length;i++) {
				System.out.println(arr[i]);
			}
	}
}
